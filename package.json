{
  "name": "automation-vue",
  "version": "2.4.4",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "node ./build-extension.js",
    "lint": "vue-cli-service lint",
    "release": "standard-version",
    "pushtag": "git push --follow-tags origin master"
  },
  "gitHooks": {
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "dependencies": {
